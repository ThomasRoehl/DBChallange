<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="20dp"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="de.throehl.mobilitaetsprofil.view.FoundConnectionsActivity"
    tools:showIn="@layout/activity_found_connections">

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/linearLayout2">

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:weightSum="1"
            android:layout_gravity="top|center"
            android:layout_row="0"
            android:layout_column="10"
            android:layout_below="@+id/section_label"
            android:layout_alignParentStart="true"
            android:id="@+id/linearLayout">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="von"
                android:id="@+id/textView" />


            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Start"
                android:id="@+id/id_start_position"
                android:background="@color/colorGray1"
                android:layout_below="@+id/linearLayout"
                android:layout_alignEnd="@+id/linearLayout"
                android:layout_alignParentStart="true"
                />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="nach"
                android:id="@+id/textView2" />



            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Destination"
                android:id="@+id/id_destination_position"
                android:background="@color/colorGray1"
                android:layout_below="@+id/linearLayout"
                android:layout_alignEnd="@+id/linearLayout"
                android:layout_alignParentStart="true" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/id_leere_Zeile" />


        </LinearLayout>


    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_marginTop="20dp"
        android:layout_below="@+id/linearLayout2"
        android:id="@+id/id_lLayout_1"
        android:clickable="true">

        <GridLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="10dp"
            android:layout_below="@+id/linearLayout"
            android:layout_alignParentStart="true"
            android:clickable="false" >

            <TextView
                android:layout_width="25dp"
                android:layout_height="wrap_content"
                android:text="Ab: "
                android:id="@+id/textView4"
                android:layout_row="0"
                android:layout_column="0"
                android:clickable="false" />

            <TextView
                android:layout_width="60dp"
                android:layout_height="wrap_content"
                android:text="Zeit"
                android:background="@color/colorGray1"
                android:id="@+id/time1"
                android:layout_row="0"
                android:layout_column="1"
                android:gravity="center_vertical|center_horizontal"
                android:layout_marginRight="15dp"
                android:clickable="false"
                />


            <TextView
                android:layout_width="25dp"
                android:layout_height="wrap_content"
                android:text="An:"
                android:id="@+id/textView6"
                android:layout_row="0"
                android:layout_column="3"
                android:clickable="false" />

            <TextView
                android:layout_width="60dp"
                android:layout_height="wrap_content"
                android:text="Zeit"
                android:background="@color/colorGray1"
                android:id="@+id/time1b"
                android:layout_row="0"
                android:layout_column="4"
                android:gravity="center_vertical|center_horizontal"
                android:layout_marginRight="40dp"
                android:clickable="false" />

            <TextView
                android:layout_width="50dp"
                android:layout_height="wrap_content"
                android:text="Datum:"
                android:id="@+id/textView5"
                android:layout_row="0"
                android:layout_column="6"
                android:clickable="false" />

            <TextView
                android:layout_width="80dp"
                android:layout_height="wrap_content"
                android:text="Datum"
                android:background="@color/colorGray1"
                android:id="@+id/date1"
                android:layout_row="0"
                android:layout_column="7"
                android:gravity="center_vertical|center_horizontal"
                android:clickable="false" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/destination_example_1_1"
                android:id="@+id/dest1"
                android:layout_row="2"
                android:layout_column="0"
                android:layout_columnSpan="8"
                android:clickable="false" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/destination_example_1_2"
                android:id="@+id/path1"
                android:layout_row="3"
                android:layout_column="0"
                android:layout_columnSpan="8"
                android:clickable="false" />


        </GridLayout>

    </LinearLayout>

    <GridLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/id_lLayout_1"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="10dp"
        android:layout_alignParentStart="true"
        android:id="@+id/id_gLayout_1"
        android:layout_alignParentEnd="true"
        android:clickable="true">

        <TextView
            android:layout_width="25dp"
            android:layout_height="wrap_content"
            android:text="Ab: "
            android:id="@+id/textView10"
            android:layout_row="0"
            android:layout_column="0"
            android:clickable="false"/>

        <TextView
            android:layout_width="60dp"
            android:layout_height="wrap_content"
            android:text="Zeit"
            android:background="@color/colorGray1"
            android:id="@+id/time2"
            android:layout_row="0"
            android:layout_column="1"
            android:gravity="center_vertical|center_horizontal"
            android:layout_marginRight="15dp"
            android:clickable="false"/>

        <TextView
            android:layout_width="25dp"
            android:layout_height="wrap_content"
            android:text="An:"
            android:id="@+id/textView12"
            android:layout_row="0"
            android:layout_column="3"
            android:clickable="false"/>

        <TextView
            android:layout_width="60dp"
            android:layout_height="wrap_content"
            android:text="Zeit"
            android:background="@color/colorGray1"
            android:id="@+id/time2b"
            android:layout_row="0"
            android:layout_column="4"
            android:gravity="center_vertical|center_horizontal"
            android:layout_marginRight="40dp"
            android:clickable="false"/>

        <TextView
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:text="Datum:"
            android:id="@+id/textView14"
            android:layout_row="0"
            android:layout_column="6"
            android:clickable="false"/>

        <TextView
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:text="Datum"
            android:background="@color/colorGray1"
            android:id="@+id/date2"
            android:layout_row="0"
            android:layout_column="7"
            android:gravity="center_vertical|center_horizontal"
            android:clickable="false"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/destination_example_1_1"
            android:id="@+id/dest2"
            android:layout_row="2"
            android:layout_column="0"
            android:layout_columnSpan="8"
            android:clickable="false"/>


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/destination_example_1_2"
            android:id="@+id/path2"
            android:layout_row="3"
            android:layout_column="0"
            android:layout_columnSpan="8"
            android:clickable="false"/>

    </GridLayout>

    <GridLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/id_gLayout_1"
        android:layout_marginTop="30dp"
        android:layout_marginBottom="30dp"
        android:layout_centerHorizontal="true"
        android:id="@+id/id_gLayout_2"
        android:clickable="true">


        <TextView
            android:layout_width="25dp"
            android:layout_height="wrap_content"
            android:text="Ab: "
            android:id="@+id/textView18"
            android:layout_row="0"
            android:layout_column="0"
            android:clickable="false"/>

        <TextView
            android:layout_width="60dp"
            android:layout_height="wrap_content"
            android:text="Zeit"
            android:background="@color/colorGray1"
            android:id="@+id/time3"
            android:layout_row="0"
            android:layout_column="1"
            android:gravity="center_vertical|center_horizontal"
            android:layout_marginRight="15dp"
            android:clickable="false"/>

        <TextView
            android:layout_width="25dp"
            android:layout_height="wrap_content"
            android:text="An:"
            android:id="@+id/textView20"
            android:layout_row="0"
            android:layout_column="3"
            android:clickable="false"/>

        <TextView
            android:layout_width="60dp"
            android:layout_height="wrap_content"
            android:text="Zeit"
            android:background="@color/colorGray1"
            android:id="@+id/time3b"
            android:layout_row="0"
            android:layout_column="4"
            android:gravity="center_vertical|center_horizontal"
            android:layout_marginRight="40dp"
            android:clickable="false"/>

        <TextView
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:text="Datum:"
            android:id="@+id/textView22"
            android:layout_row="0"
            android:layout_column="6"
            android:clickable="false"/>

        <TextView
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:text="Datum"
            android:background="@color/colorGray1"
            android:id="@+id/date3"
            android:layout_row="0"
            android:layout_column="7"
            android:gravity="center_vertical|center_horizontal"
            android:clickable="false"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/destination_example_1_1"
            android:id="@+id/dest3"
            android:layout_row="2"
            android:layout_column="0"
            android:layout_columnSpan="8" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/destination_example_1_2"
            android:id="@+id/path3"
            android:layout_row="3"
            android:layout_column="0"
            android:layout_columnSpan="8"
            android:clickable="false"/>
    </GridLayout>




</RelativeLayout>
